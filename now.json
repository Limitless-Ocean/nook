{
  "name": "nook",
  "version": 2,
  "build": {
    "env": {
      "REDDIT_OAUTH_ID": "@nook-reddit-oauth-id",
      "REDDIT_OAUTH_SECRET": "@nook-reddit-oauth-secret",
      "JWT_SECRET": "@nook-jwt-secret",
      "FAUNA_SECRET": "@nook-fauna-secret",
      "OAUTH_STATE": "@nook-oauth-state",
      "DISCORD_OAUTH_ID": "@nook-discord-oauth-id",
      "DISCORD_OAUTH_SECRET": "@nook-discord-oauth-secret",
      "S3_ACCESS_KEY_ID": "@nook-s3-access-key-id",
      "S3_SECRET_ACCESS_KEY": "@nook-s3-secret-access-key",
      "S3_BUCKET_NAME": "@nook-s3-bucket-name"
